<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ferechamitbeyli.mobileprogramminghw2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MobileProgrammingHW2">
        <activity android:name=".view.AddActivity"
            android:parentActivityName=".view.MainActivity"></activity>
        <activity
            android:name=".view.ReportActivity"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".view.DetailActivity"
            android:parentActivityName=".view.MainActivity" />
        <activity
            android:name=".view.EditActivity"
            android:parentActivityName=".view.MainActivity" />
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>